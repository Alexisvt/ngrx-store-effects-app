{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": [false],
    "only-arrow-functions": [false],
    "no-console": [false],
    "ordered-imports": [false],
    "ban-types": [false],
    "arrow-parens": [false],
    "max-classes-per-file": [false],
    "member-access": [false],
    "object-literal-sort-keys": [false]
  },
  "rulesDirectory": []
}
